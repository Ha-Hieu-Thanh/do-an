{
  "name": "common",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:client:dev": "nest start --watch client",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/common/test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js -d ./libs/database-type-orm/src/data-source-task-manager.ts",
    "migration:generate": "npm run typeorm -- migration:generate ./libs/database-type-orm/src/migrations/task-manager-migration",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npx typeorm-ts-node-esm migration:revert -d ./libs/database-type-orm/src/data-source-task-manager.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.348.0",
    "@casl/ability": "^6.5.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^9.4.3",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/platform-express": "^9.4.2",
    "@nestjs/platform-socket.io": "^9.4.3",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.4.3",
    "@socket.io/redis-adapter": "^8.2.1",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.4",
    "cache-manager": "^5.2.1",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "moment": "^2.29.4",
    "mongoose": "^7.5.4",
    "mysql2": "^3.3.2",
    "nodemailer": "^6.9.3",
    "redis": "^4.6.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.2.0",
    "sharp": "^0.32.1",
    "string-format": "^2.0.0",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/lodash": "^4.14.194",
    "@types/md5": "^2.3.2",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.8",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.8.8",
    "socket.io-client": "^4.7.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/core(|/.*)$": "<rootDir>/libs/core/src/$1",
      "^@app/helpers(|/.*)$": "<rootDir>/libs/helpers/src/$1",
      "^@app/database-type-orm(|/.*)$": "<rootDir>/libs/database-type-orm/src/$1",
      "^@app/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@app/error-message(|/.*)$": "<rootDir>/libs/error-message/src/$1",
      "^@app/cache(|/.*)$": "<rootDir>/libs/cache/src/$1",
      "^@app/jwt-authentication(|/.*)$": "<rootDir>/libs/jwt-authentication/src/$1",
      "^@app/log-action(|/.*)$": "<rootDir>/libs/log-action/src/$1",
      "^@app/send-mail(|/.*)$": "<rootDir>/libs/send-mail/src/$1",
      "^@app/queue(|/.*)$": "<rootDir>/libs/queue/src/$1",
      "^@app/template-mail(|/.*)$": "<rootDir>/libs/template-mail/src/$1",
      "^@app/s3-upload(|/.*)$": "<rootDir>/libs/s3-upload/src/$1",
      "^@app/authorization(|/.*)$": "<rootDir>/libs/authorization/src/$1",
      "^@app/database-mongodb(|/.*)$": "<rootDir>/libs/database-mongodb/src/$1",
      "^@app/message-provider(|/.*)$": "<rootDir>/libs/message-provider/src/$1"
    }
  }
}